{
  "name": "{{ appName }}",
  "version": "1.0.0",
  "private": true,
  "description": "{{{ description }}}",
  "author": "{{{ author }}}",
  "license": "ISC",
  "homepage": "https://github.com/{{ owner }}/{{ repo }}",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "start": "probot run ./lib/index.js",
    "test": "jest",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "probot run ./lib/index.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "probot": "^11.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.19",
    "concurrently": "^5.3.0",
    "jest": "^26.6.3",
    "nock": "^13.0.5",
    "smee-client": "^1.2.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
